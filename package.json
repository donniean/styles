{
  "name": "@donniean/styles",
  "version": "1.5.0",
  "description": "opinionated styles",
  "homepage": "https://github.com/donniean/styles#readme",
  "bugs": {
    "url": "https://github.com/donniean/styles/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/donniean/styles.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "main": "",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx postcss src -d dist --env production --ext min.css",
    "commit": "cz",
    "lint:cspell": "cspell lint --no-progress --relative --no-must-find-files --dot --gitignore \"**\"",
    "lint:eslint": "npx eslint \"**/*.{js,cjs}\"",
    "lint:eslint:fix": "npx eslint --fix \"**/*.{js,cjs}\"",
    "lint:htmlhint": "htmlhint \"**/*.html\"",
    "lint:markdownlint": "markdownlint --fix \"**/*.md\"",
    "lint:prettier": "prettier --check --ignore-unknown \"**/*.{js,cjs,json,html,css,md,yaml,yml}\"",
    "lint:prettier:fix": "prettier --write --ignore-unknown \"**/*.{js,cjs,json,html,css,md,yaml,yml}\"",
    "lint:stylelint": "stylelint \"**/*.css\"",
    "lint:stylelint:fix": "stylelint --fix \"**/*.css\"",
    "pre-commit": "lint-staged --concurrent false",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "sort-package-json": "sort-package-json"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "commitizen": "^4.3.0",
    "cspell": "^6.31.1",
    "cssnano": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "htmlhint": "^1.1.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "markdownlint-cli": "^0.33.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.7",
    "semantic-release": "^21.0.1",
    "sort-package-json": "^2.4.1",
    "stylelint": "^15.5.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^33.0.0"
  }
}
