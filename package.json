{
  "name": "@donniean/styles",
  "version": "1.4.1",
  "description": "",
  "homepage": "https://github.com/donniean/styles#readme",
  "bugs": {
    "url": "https://github.com/donniean/styles/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniean/styles.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "main": "",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx postcss src -d dist --env production --ext min.css",
    "commit": "git-cz",
    "lint:cspell": "cspell --no-must-find-files \"**\"",
    "lint:eslint": "npx eslint \"**/*.js\"",
    "lint:eslint:fix": "npx eslint --fix \"**/*.js\"",
    "lint:stylelint": "npx stylelint \"**/*.{css,scss}\"",
    "lint:stylelint:fix": "npx stylelint --fix \"**/*.{css,scss}\"",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "prettier": "npx prettier --write \"**/*.{js,css,scss,json,md}\"",
    "pub": "npm run release && npm publish --access public",
    "release": "npm run build && standard-version"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "commitizen": "^4.2.5",
    "cspell": "^6.14.1",
    "cssnano": "^5.1.14",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "stylelint": "^14.14.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0"
  }
}
