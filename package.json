{
  "name": "@donniean/styles",
  "version": "1.2.0",
  "description": "",
  "homepage": "https://github.com/donniean/styles#readme",
  "bugs": {
    "url": "https://github.com/donniean/styles/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniean/styles.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "main": "",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx postcss src -d dist --env production --ext min.css",
    "prettier": "npx prettier --write \"**/*.{js,css,scss,json,md}\"",
    "eslint": "npx eslint --fix \"**/*.js\"",
    "stylelint": "npx stylelint --fix \"**/*.{css,scss}\"",
    "cspell": "cspell \"**\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "commit": "git-cz",
    "release": "npm run build && standard-version",
    "pub": "npm run release && npm publish --access public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "commitizen": "^4.2.4",
    "cspell": "^5.12.2",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "postcss": "^8.1.6",
    "postcss-cli": "^8.2.0",
    "prettier": "^2.4.1",
    "standard-version": "^9.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0"
  }
}
